{
  "name": "order-history",
  "version": "0.1.0",
  "description": "Micro Frontend - Order History",
  "main": "public/index.html",
  "author": "zizcode",
  "private": true,
  "scripts": {
    "build:client": "webpack --config webpack.prod.ts",
    "build:server": "webpack --config webpack.config.server.ts",
    "client": "webpack-dev-server --config webpack.dev.ts",
    "dev": "concurrently \"nodemon\" \"npm run client\"",
    "build": "concurrently --kill-others-on-fail \"npm run build:server\" \"npm run build:client\"",
    "start": "node -r dotenv/config ./dist/server/server.js",
    "lint": "eslint --ext .ts --ext .vue src",
    "lint:fix": "eslint --ext .ts --ext .vue src --fix",
    "prettier": "prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "release": "standard-version"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-winston": "^4.2.0",
    "http-status-codes": "^2.1.4",
    "joi": "^17.6.0",
    "lightship": "^6.7.2",
    "querystring": "^0.2.1",
    "typescript": "^4.6.3",
    "vue": "^3.0.0",
    "vue-router": "^4.0.14",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/node": "^7.16.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.9",
    "@babel/plugin-proposal-private-methods": "^7.16.11",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/fork-ts-checker-webpack-plugin": "^0.4.5",
    "@types/joi": "^17.2.3",
    "@types/node": "^16.11.4",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^4.0.3",
    "@types/webpack-node-externals": "^2.5.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.29.3",
    "@vue/compiler-sfc": "^3.2.31",
    "autoprefixer": "^10.3.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^6.2.1",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.6.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.3.2",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.0",
    "lint-staged": "^11.1.2",
    "mini-css-extract-plugin": "^2.2.0",
    "node-sass": "^6.0.1",
    "path": "^0.12.7",
    "postcss": "^8.4.6",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.3.2",
    "sass-loader": "^12.4.0",
    "standard-version": "^9.3.1",
    "style-loader": "^3.2.1",
    "tailwindcss": "^2.2.19",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "uninstall": "0.0.0",
    "url-loader": "^4.1.1",
    "vue-eslint-parser": "^8.3.0",
    "vue-loader": "^16.3.0",
    "vue-style-loader": "^4.1.3",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-manifest-plugin": "^4.0.2",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
